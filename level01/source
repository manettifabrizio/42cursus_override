#include <stdio.h>
#include <stdbool.h>

extern unsigned int a_user_name;

int verify_user_name()
{
    int __saved_esi;
    bool c = &__saved_esi < 16;
    bool z = &__saved_esi == 16;
    int ecx = 7;
    char *esi = &a_user_name;
    char *edi = "dat_wil";

    puts("verifying username....\n");
    while (ecx != 0)
    {
        char temp1_1 = *esi;
        char temp2_1 = *edi;

        c = temp1_1 < temp2_1;
        z = temp1_1 == temp2_1;
        esi = &esi[1];
        edi++;
        ecx--;
        if ((!z))
        {
            break;
        }
    }
    return (((!z) && (!c)) - c);
}

int verify_user_pass(char *pass)
{
    char *esi = pass;
    char *edi = "admin";
    int ecx = 5;
    bool c, z;

    while (ecx != 0)
    {
        char tmp0 = *esi;
        char tmp1 = *edi;

        c = tmp0 < tmp1;
        z = tmp0 == tmp1;
        esi = &esi[1];
        edi++;
        ecx--;
        if ((!z))
            break;
    }
    return (((!z) && (!c)) - c);
}

extern char a_user_name;

