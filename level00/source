#include <stdio.h>

int main()
{
    int buf[4];

    puts("***********************************");
    puts("* \t     -Level00 -\t\t  *");
    puts("***********************************");
    printf("Password:");
    scanf("%d", &buf);
    if (buf[0] != 5276)
    {
        puts("\nInvalid Password!");
        return 1;
    }
    puts("\nAuthenticated!");
    system("/bin/sh");
    return 0;
}
